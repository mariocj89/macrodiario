{
  "expo": {
    "name": "Macro Diario",
    "slug": "macrodiario",
    "version": "1.23.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#b7e26f"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.macrodiario"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#b7e26f"
      },
      "package": "com.macrodiario2"
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "extra": {
      "eas": {
        "projectId": "aedb2798-dae1-4090-b99e-e14caf6bac80"
      }
    },
    "owner": "etcaterva",
    "plugins": [
      "expo-localization"
    ]
  }
}
